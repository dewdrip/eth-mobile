{
  "expo": {
    "name": "ETH Mobile",
    "slug": "eth-mobile",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/icons/logo.png",
    "scheme": "ethmobile",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "ETH Mobile needs to access your Camera",
        "NSFaceIDUsageDescription": "ETH Mobile needs to access your Face ID",
        "NSLocationWhenInUseUsageDescription": "ETH Mobile needs to access your Location",
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.dewdrip.ethmobile",
      "icon": {
        "light": "./assets/icons/ios-light.png",
        "dark": "./assets/icons/ios-dark.png",
        "tinted": "./assets/icons/ios-tinted.png"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/adaptive-icon.png",
        "monochromeImage": "./assets/icons/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.dewdrip.ethmobile",
      "intentFilters": [
        {
          "autoVerify": true,
          "action": "VIEW",
          "data": {
            "scheme": "https",
            "host": "ethmobile.io"
          },
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/icons/logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 26
          },
          "ios": {
            "extraPods": [
              {
                "name": "OpenSSL-Universal",
                "configurations": ["Release", "Debug"],
                "modular_headers": true,
                "version": "3.3.2000"
              }
            ]
          }
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/splash-icon-dark.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "image": "./assets/icons/splash-icon-light.png",
            "imageWidth": 200,
            "resizeMode": "contain",
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "65d7b5ca-83fe-497c-bbfd-e30071e62002"
      }
    },
    "owner": "dewdrip"
  }
}
